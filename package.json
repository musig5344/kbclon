{
  "name": "kb-starbanking-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/cli": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/device": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@supabase/supabase-js": "^2.52.0",
    "@tanstack/react-query": "^5.84.0",
    "@tanstack/react-query-devtools": "^5.84.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "dev": "craco start",
    "start": "craco start",
    "build": "craco build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --verbose",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci",
    "lint": "eslint src --ext .ts,.tsx --fix --max-warnings 0",
    "lint:check": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit --strict",
    "type-check:watch": "tsc --noEmit --strict --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf build node_modules/.cache",
    "prepare": "husky install || true",
    "clean:all": "rm -rf build node_modules/.cache node_modules && npm install",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:ci",
    "pre-commit": "npm run lint:fix && npm run format",
    "quality-check": "npm run type-check && npm run lint:check && npm run format:check",
    "eject": "react-scripts eject",
    "android": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:dev": "npm run build && npx cap sync android && npx cap open android",
    "ios": "npm run build && npx cap sync ios && npx cap open ios",
    "apk": "sh android-build.sh",
    "images:optimize": "node scripts/image-optimization.js",
    "images:analyze": "node scripts/image-analysis.js",
    "images:webp": "node scripts/webp-converter.js",
    "images:sprites": "node scripts/sprite-generator.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.0",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "prettier": "^3.0.0",
    "sharp": "^0.33.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
